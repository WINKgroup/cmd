  },
  "homepage": "https://github.com/WINKgroup/cmd#readme",
  "dependencies": {
    "@winkgroup/console-log": "^3.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.3",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "type-fest": "^3.12.0",
    "typescript": "^5.1.6"
  }
